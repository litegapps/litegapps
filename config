# list-restore = lite,core,go,micro,pixel,nano,basic
litegapps.restore=lite
# list-type = lite,core,go,micro,pixel,nano,basic
litegapps.type=lite
# litegapps multi or single files.tar archive
# This function is used to create a single .tar file and is used for many variants. This speeds up the build process for many variants of pixel, micro, core, and lite.
litegapps.tar=multi

# Litegappsx prop
